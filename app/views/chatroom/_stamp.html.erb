<% stamps = YAML.load_file(Rails.root.join('app', 'assets', 'config', 'stamp.yml')) %>
   
    <% stamps['stamps'].each do |key, stamp| %>
        <div class="stamp">
            <%= image_tag asset_path(stamp['url']), 
                          alt: stamp['name'], 
                          data: { id: key},
                          class: 'stamp-item' %>
        </div>
    <% end %>

